{
  "name": "nextauth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:export": "next build && next export",
    "build:css": "tailwindcss -i ./src/tailwind.css -o ./styles/build.css",
    "watch:css": "tailwindcss -i ./src/tailwind.css -o ./styles/build.css --watch",
    "sass:globals": "sass ./src/globals.scss ./styles/globals.css ",
    "sass:globals:watch": "sass ./src/globals.scss ./styles/globals.css --watch ",
    "build:bs": "sass --style expanded --source-map --embed-sources --no-error-css ./src/bootstrap.scss:./styles/bootstrap.css ",
    "build:bs:watch": "sass --style expanded --source-map --embed-sources --no-error-css ./src/bootstrap.scss:./styles/bootstrap.css --watch "
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.0.3",
    "bootstrap": "^5.1.3",
    "framer-motion": "^6.3.11",
    "mongodb": "^4.7.0",
    "next": "12.1.6",
    "next-auth": "^4.5.0",
    "nodemailer": "^6.7.5",
    "react": "18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "html5-boilerplate": "^8.0.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.14",
    "sass": "^1.52.3",
    "tailwindcss": "^3.1.3"
  }
}
